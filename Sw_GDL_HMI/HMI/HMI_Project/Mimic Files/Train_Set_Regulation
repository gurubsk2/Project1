	''End Sub
	
	'* *******************************************************************************************
	'* Manage the click on OK button
	'* *******************************************************************************************
	''Private Sub Button_1_Click()
	''On Error GoTo ErrorHandler
	''
	''    Dim sInterposeCmd As String
	''
	''
	''        If (m_iCurTrainID <> "" And m_OPCSetItemCurRegulationStatus.Value <> 0) Then
